{
  "name": "floralvault-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node ./src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "seed": "ts-node prisma/seed.ts",
    "prisma:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=src/prisma/schema.prisma",
    "render-build": "npm run prisma:generate && npx prisma db push --schema=src/prisma/schema.prisma --accept-data-loss --skip-generate && npm run build"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "6.5.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "firebase-admin": "^13.6.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^3.2.2",
    "nanoid": "^5.1.5",
    "node-cron": "^4.2.1",
    "pg": "^8.14.1",
    "resend": "^6.9.2",
    "slugify": "^1.6.6",
    "stripe": "^20.3.1",
    "uploadthing": "^7.7.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^22.15.2",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.14",
    "nodemon": "^3.1.9",
    "prisma": "6.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
